<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.mydemo" >


    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".swipe.SwipeRefreshActivity"/>
        <activity android:name=".enu.EnumActivity"/>
        <activity android:name=".permission.PermissionActivity"/>
        <activity android:name=".design.DesignActivity"/>
        <activity android:name=".recItem.RecActivity"/>

    </application>

</manifest>
